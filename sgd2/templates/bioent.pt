<script>
	$(document).ready(function() {
    	$('#interaction_table').dataTable( {
    		"sPaginationType": "bootstrap",
    		"aaSorting": [[2, "desc"]]
  		} );
  		$('#phenotype_table').dataTable( {
    		"sPaginationType": "bootstrap",
    		"aaSorting": [[1, "desc"]]
  		} );
	} );
</script>

</div>

</body>
</div>
</div>

