<div metal:use-macro="layout">
  <div metal:fill-slot="content">


<body>
	
<!-- Subhead
================================================== -->
<header class="jumbotron subhead" id="overview">
	<div class="container">
		<br></br>
		<h1><span tal:content="bioent.full_name"></h1>
		<p class="lead">
			<span tal:content="bioent.description"></span>
		</p>
	</div>
</header>


<div class="container-fluid">

<!-- Docs nav
================================================== -->
<div class="row-fluid">
	<div id='sidebar' class="span3 bs-docs-sidebar">
		<ul class="nav nav-list bs-docs-sidenav affix-top" id='navbar' name='navbar' style="margin-left:auto; margin-right:auto">
			<li><h3><span tal:content="bioent.full_name"></h3></li>
			<li><a href="#summary"><i class="icon-chevron-right"></i> Summary</a></li>
			<li><a href="#location"><i class="icon-chevron-right"></i> Sequence and Location</a></li>
          	<li><a href="#go"><i class="icon-chevron-right"></i> Gene Ontology</a></li>
          	<li><a href="#phenotype"><i class="icon-chevron-right"></i> Phenotype</a></li>
          	<li><a href="#interactions"><i class="icon-chevron-right"></i>Interaction</a></li>
          	<li><a href="#expression"><i class="icon-chevron-right"></i> Expression</a></li>
          	<li><a href="#wiki"><i class="icon-chevron-right"></i> Wiki</a></li>
			<li><a href="#history"><i class="icon-chevron-right"></i> History</a></li>
          	<li><a href="#literature"><i class="icon-chevron-right"></i> References</a></li>
		</ul>
	</div>
	
	<div class="span9">  

<!-- Summary
================================================== -->
<section id="summary">
	<div class="page-header">
		<h1>Summary</h1>
		<div>
			<ul class='unstyled'>
				<li tal:condition="python: len(bioent.aliases) > 0"><strong>Aliases: </strong><span tal:content="bioent.alias_str"></li>
				<li tal:condition="python: bioent.bioent_type"><strong>Bioent-Type: </strong><span tal:content="bioent.bioent_type"></li>
				<li tal:condition="python: bioent.qualifier"><strong>Qualifier: </strong><span tal:content="bioent.qualifier"></li>
				<br>
				<li tal:condition="python: bioent.source"><strong>Source: </strong><span tal:content="bioent.source"></li>
				<li tal:condition="python: bioent.status"><strong>Status: </strong><span tal:content="bioent.status"></li>
				<li tal:condition="python: bioent.attribute"><strong>Attribute: </strong><span tal:content="bioent.attribute"></li>
				<li tal:condition="python: bioent.name_description"><strong>Name Derived From: </strong><span tal:content="bioent.name_description"></li>
			</ul>
		</div>
	</div>
</section>

<!-- Sequence and Location
================================================== -->
<section id="location">
	<div class="page-header">
		<h1>Sequence and Location</h1>
		
		<!-- Brief
		========================================== -->
		<div class="container">
			<ul class='unstyled'>
				<li tal:condition="python: bioent.genetic_position"><strong>Genetic Position: </strong><span tal:content="bioent.genetic_position"></li>
			</ul>
		</div>

		<!-- In-depth
		========================================== 
		<p class="collapse" id="viewdetails_location">
           	This is the in-depth chromosomal location information. </p>
		<p><a class="btn" id="collapse_btn" name="collapse_btn" data-toggle="collapse" data-target="#viewdetails_location">View details &raquo;</a></p>
        -->
	</div>
</section>

<!-- GO
================================================== -->
<section id="go">
	<div class="page-header">
		<h1>Gene Ontology <span id='go_header'></span></h1>
		<a tal:attributes="href bioent.go_evidence_link"><button class="btn btn-large btn-warning">All Gene Ontology Evidence <i class="icon-chevron-right icon-white"></i></button></a>
		<br></br>
		
		<h3 style="float:left;">Molecular Function <span id='go_f_header'></span></h3>
		<span id='go_f_message' class="hide">No known molecular function gene ontology associations.</span>
		<div id='go_f_wrapper'>	
			<ul class='unstyled'>
				<table class='table table-striped table-bordered table-condensed tablesorter' id='go_f_table' width="100%">
			<thead>
  			<tr>
    			<th>GO Term</th>
    			<th>Gene</th>
    			<th>Gene Description</th>
    			<th>Evidence Codes</th>
  			</tr>
  			</thead>
  			
			</table>
			</ul>
		</div>		
		<br></br>

		<h3 style="float:left;">Biological Process <span id='go_p_header'></span></h3>
		<span id='go_p_message' class="hide">No known biological process gene ontology associations.</span>
		<div id='go_p_wrapper'>	
			<ul class='unstyled'>
				<table class='table table-striped table-bordered table-condensed tablesorter' id='go_p_table' width="100%">
			<thead>
  			<tr>
    			<th>GO Term</th>
    			<th>Gene</th>
    			<th>Gene Description</th>
    			<th>Evidence Codes</th>
  			</tr>
  			</thead>
  			
			</table>
			</ul>
		</div>	
		<br></br>
		
		<h3 style="float:left;">Cellular Component <span id='go_c_header'></span></h3>
		<span id='go_c_message' class="hide">No known cellular component gene ontology associations.</span>
		<div id='go_c_wrapper'>	
			<ul class='unstyled'>
				<table class='table table-striped table-bordered table-condensed tablesorter' id='go_c_table' width="100%">
			<thead>
  			<tr>
    			<th>GO Term</th>
    			<th>Gene</th>
    			<th>Gene Description</th>    			
    			<th>Evidence Codes</th>
  			</tr>
  			</thead>
  			
			</table>
			</ul>
		</div>
		<br></br>
		<h3>Computational Curations <span id='go_comp_header'></span></h3>
		<p>There are <span id='go_comp_message'></span> computational go terms curated for <span tal:content="bioent.name"></span>.</p>

	</div>
</section>

<!-- Phenotype
================================================== -->
<section id="phenotype">
	<div class="page-header">
		<h1>Phenotype <span id='phenotype_header'></span></h1> 
		<a tal:attributes="href bioent.all_link('bioent_biocon', 'phenotype')"><button class="btn btn-large btn-warning">All Phenotype Evidence <i class="icon-chevron-right icon-white"></i></button></a>
		<br></br>
		<!-- Phenotype Table
		========================================== -->
		<h3 style="float:left;">Cellular Phenotype <span id='cellular_phenotype_header'></span></h3>
		<div>
			<span id='phenotype_message' class="hide">No known phenotypes.</span>
			<div id='phenotype_wrapper'>
				<table class='table table-striped table-bordered table-condensed tablesorter' id='phenotype_table' width="100%">
				<thead>
  				<tr>
  					<th>Phenotype</th>
  					<th>Qualifier</th>
  					<th>Mutant Type</th>
  				</tr>
  				</thead>
				</table>
			</div>
		</div>
		<br></br>
		<!-- Chemical Phenotype Table
		========================================== -->
		<h3 style="float:left;">Chemical Phenotype <span id='chemical_phenotype_header'></span></h3>
		<div>
			<span id='chemical_phenotype_message'  class="hide">No known chemical phenotypes.</span>
			<div id='chemical_phenotype_wrapper'>
								<table class='table table-striped table-bordered table-condensed tablesorter' id='chemical_phenotype_table' width="100%">
			<thead>
  			<tr>
   				<th>Phenotype</th>
  				<th>Chemical</th>
  				<th>Qualifier</th>  				
  				<th>Mutant Type</th>
  			</tr>
  			</thead>
 
			</table>
		</div>
		<br></br>
		<!-- Protein/Peptide and RNA Interactions Table
		========================================== -->
		<h3 style="float:left;">Protein/Peptide and RNA Phenotype <span id='pp_rna_phenotype_header'></span></h3>
		<div>
			<span id='pp_rna_phenotype_message' class="hide">No known protein/peptide and RNA phenotypes.</span>
			<div id='pp_rna_phenotype_wrapper'>				
				<table class='table table-striped table-bordered table-condensed tablesorter' id='pp_rna_phenotype_table' width="100%">
			<thead>
  			<tr>
  				<th>Phenotype</th>
  				<th>Reporter</th>
  				<th>Qualifier.</th>
  				<th>Mutant Type</th>
  			</tr>
  			</thead>
 
			</table>
		</div>

	</div>
</section>

<!-- Interactions
================================================== -->
<section id="interactions">
	<div class="page-header">
		<h1>Interaction <span id='interaction_header'></span></h1>
		<a tal:attributes="href bioent.all_link('biorel', 'interaction')"><button class="btn btn-large btn-warning">All Interaction Evidence <i class="icon-chevron-right icon-white"></i></button></a>
		<br></br>
		<h3>Cytoscape Visualization</h3>
		<strong>Filter Interactions by # Total Evidence:</strong>
		<br></br>
		<div id="slider-range-min"></div>
		<br></br>
		<div id="cytoscapeweb">
            Cytoscape visualization is loading...
        </div>
        
		<span id='interaction_message' class="hide">No known interactions.</span>
		<div id='interaction_wrapper'>					
			<ul class='unstyled'>
				<table class='table table-striped table-bordered table-condensed tablesorter' id='interaction_table' width="100%">
			<thead>
  			<tr>
    			<th>Interactor</th>
    			<th>Genetic Evidence</th>
    			<th>Physical Evidence</th>
    			<th>Total Evidence</th>
  			</tr>
  			</thead>
  			
			</table>
			</ul>
		</div>

	</div>
</section>

<!-- Expression
================================================== -->
<section id="expression">
	<div class="page-header">
		<h1>Expression</h1>
		
		<!-- Brief
		========================================== -->
		<p>This is expression information.</p>

		<!-- In-depth
		========================================== 
		<p class="collapse" id="viewdetails_expression">
           	This is the in-depth expression information. </p>
		<p><a class="btn" id="collapse_btn" name="collapse_btn" data-toggle="collapse" data-target="#viewdetails_expression">View details &raquo;</a></p>
		-->
	</div>
</section>

<!-- Wiki
================================================== -->
<section id="wiki">
	<div class="page-header">
		<h1>Wiki</h1>
    	<span tal:content="structure bioent.wiki_name_with_link"></span>
	</div>
</section>

<!-- History
================================================== -->
<section id="history">
	<div class="page-header">
		<h1>History</h1>
		
		<!-- Brief
		========================================== -->
		<p>This is history information.</p>

		<!-- In-depth
		========================================== 
		<p class="collapse" id="viewdetails_history">
           	This is the in-depth history information. </p>
		<p><a class="btn" id="collapse_btn" name="collapse_btn" data-toggle="collapse" data-target="#viewdetails_history">View details &raquo;</a></p>
        -->
	</div>
</section>

<!-- References
================================================== -->
<section id="literature">
	<div class="page-header">
		<h1>References</h1>
		
		<!-- Brief
		========================================== -->
		<p>This is literature information.</p>

		<!-- In-depth
		========================================== 
		<p class="collapse" id="viewdetails_literature">
           	This is the in-depth literature information. </p>
		<p><a class="btn" id="collapse_btn" name="collapse_btn" data-toggle="collapse" data-target="#viewdetails_literature">View details &raquo;</a></p>
		-->
	</div>
</section>

<script>
	$(document).ready(function() {

		//Get go data
  		$.getJSON('${bioent.go_link}', function(data) {
			var go_c = data['go_c'];
  			var go_p = data['go_p'];
  			var go_f = data['go_f'];
  			var comp_count = data['computational'];
  			
			//Set total go count
			var num_rows = go_c.length + go_p.length + go_f.length;
			document.getElementById("go_header").innerHTML = '(' + num_rows + ')';
			
  			//Set up function table
			var options = basic_datatable_options("go_f_header", '${bioent.go_f_filename}', "go_f_wrapper", "go_f_message");
			options["aaSorting"] = [[0, "asc"]];
			options["aaData"] = go_f;
			options["aoColumns"] = [null, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, null]
			$('#go_f_table').dataTable(options);
			
			//Set up process table
			var options = basic_datatable_options("go_p_header", '${bioent.go_p_filename}', "go_p_wrapper", "go_p_message");
			options["aaSorting"] = [[0, "asc"]];
			options["aaData"] = go_p;
			options["aoColumns"] = [null, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, null]
			$('#go_p_table').dataTable(options);
		
			//Set up component table
			var options = basic_datatable_options("go_c_header", '${bioent.go_c_filename}', "go_c_wrapper", "go_c_message");
			options["aaSorting"] = [[0, "asc"]];
			options["aaData"] = go_c;
			options["aoColumns"] = [null, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, null]
			$('#go_c_table').dataTable(options);
			
			//Set up computational message
			document.getElementById("go_comp_header").innerHTML = '(' + comp_count + ')';
			document.getElementById("go_comp_message").innerHTML = comp_count;
  		});
			
  		//Get phenotype data
  		$.getJSON('${bioent.phenotype_link}', function(data) {
  			var phenotype_table = data['phenotype'];
  			var chemical_phenotype_table = data['chemical_phenotype'];
  			var pp_rna_phenotype_table = data['pp_rna_phenotype'];
  			
			//Set total phenotype count
			var num_rows = phenotype_table.length + chemical_phenotype_table.length + pp_rna_phenotype_table.length;
			document.getElementById("phenotype_header").innerHTML = '(' + num_rows + ')';
			
  			//Set up phenotype table
			var options = basic_datatable_options("cellular_phenotype_header", '${bioent.phenotype_file_name}', "phenotype_wrapper", "phenotype_message");
			options["aaSorting"] = [[0, "asc"], [1, 'asc']];
			options["aaData"] = phenotype_table;
			$('#phenotype_table').dataTable(options);
		
			//Set up chemical phenotype table
			var options = basic_datatable_options("chemical_phenotype_header", '${bioent.chemical_phenotype_file_name}', "chemical_phenotype_wrapper", "chemical_phenotype_message");
			options["aaSorting"] = [[0, "asc"]];
			options["aaData"] = chemical_phenotype_table;
			$('#chemical_phenotype_table').dataTable(options);
		
			//Set up pp-rna phenotype table
			var options = basic_datatable_options("pp_rna_phenotype_header", '${bioent.pp_rna_phenotype_file_name}', "pp_rna_phenotype_wrapper", "pp_rna_phenotype_message");
			options["aaSorting"] = [[0, "asc"]];
			options["aaData"] = pp_rna_phenotype_table;
			$('#pp_rna_phenotype_table').dataTable(options);
  		});
  		
  		//Set up interaction table
		var options = basic_datatable_options("interaction_header", '${bioent.interaction_file_name}', "interaction_wrapper", "interaction_message");
		options["aaSorting"] = [[3, "desc"]];
		options["aoColumns"] = [null, null, null, {"sType":"num-html"}];
		options["bProcessing"] = true;
		options["sAjaxSource"] = '${bioent.interaction_link}';
		options["bPaginate"] = true;

		$('#interaction_table').dataTable(options);
	} );
</script>

<script type="text/javascript">
window.onload=function() {
	setup_cytoscape_vis("${bioent.interaction_graph_link}");   
};           

</script>
        
<style>
	/* The Cytoscape Web container must have its dimensions set. */
	html, body { height: 80%; width: 100%; padding: 0; margin: 0; }
	#cytoscapeweb { width: 100%; height: 100%; }
</style>

</div>

</body>
</div>
</div>

