<div metal:use-macro="layout">
  <div metal:fill-slot="content">


<body>

<!-- Subhead
================================================== -->
<header class="jumbotron subhead" id="overview">
	<div class="container">
		<h1><span tal:content="bioent.basic_info.name"> <small>(<span tal:content="bioent.basic_info.official_name">)</small></h1>
		<p class="lead">
			<span tal:content="bioent.basic_info.description">
		</p>
	</div>
</header>


<div class="container">

<!-- Docs nav
================================================== -->
<div class="row">
	<div id='sidebar' class="span3 bs-docs-sidebar">
		<ul class="nav nav-list bs-docs-sidenav affix-top" id='navbar' name='navbar'>
			<li><h3><span tal:content="bioent.basic_info.name"> <small>(<span tal:content="bioent.basic_info.official_name">)</small></h3></li>
			<li><a href="#summary"><i class="icon-chevron-right"></i> Summary</a></li>
			<li><a href="#location"><i class="icon-chevron-right"></i> Sequence and Location</a></li>
          	<li><a href="#go"><i class="icon-chevron-right"></i> Gene Ontology</a></li>
          	<li><a href="#phenotype"><i class="icon-chevron-right"></i> Phenotype</a></li>
          	<li><a href="#interactions"><i class="icon-chevron-right"></i> Interactions</a></li>
          	<li><a href="#expression"><i class="icon-chevron-right"></i> Expression</a></li>
          	<li><a href="#wiki"><i class="icon-chevron-right"></i> Wiki</a></li>
			<li><a href="#history"><i class="icon-chevron-right"></i> History</a></li>
          	<li><a href="#literature"><i class="icon-chevron-right"></i> References</a></li>
		</ul>
	</div>
	
	<div class="span9">  

<!-- Summary
================================================== -->
<section id="summary">
	<div class="page-header">
		<h1>Summary</h1>
		<div>
			<ul class='unstyled'>
				<li tal:condition="python: bioent.basic_info.aliases"><strong>Aliases: </strong><span tal:content="bioent.basic_info.aliases"></li>
				<li tal:condition="python: bioent.basic_info.bioent_type"><strong>Bioent-Type: </strong><span tal:content="bioent.basic_info.bioent_type"></li>
				<li tal:condition="python: bioent.basic_info.qualifier"><strong>Qualifier: </strong><span tal:content="bioent.basic_info.qualifier"></li>
				<br>
				<li tal:condition="python: bioent.basic_info.source"><strong>Source: </strong><span tal:content="bioent.basic_info.source"></li>
				<li tal:condition="python: bioent.basic_info.status"><strong>Status: </strong><span tal:content="bioent.basic_info.status"></li>
				<li tal:condition="python: bioent.basic_info.attribute"><strong>Attribute: </strong><span tal:content="bioent.basic_info.attribute"></li>
				<li tal:condition="python: bioent.basic_info.name_description"><strong>Name Derived From: </strong><span tal:content="bioent.basic_info.name_description"></li>
			</ul>
		</div>
	</div>
</section>

<!-- Sequence and Location
================================================== -->
<section id="location">
	<div class="page-header">
		<h1>Sequence and Location</h1>
		
		<!-- Brief
		========================================== -->
		<div class="container">
			<ul class='unstyled'>
				<li tal:condition="python: bioent.genetic_position"><strong>Genetic Position: </strong><span tal:content="bioent.genetic_position"></li>
			</ul>
		</div>

		<!-- In-depth
		========================================== 
		<p class="collapse" id="viewdetails_location">
           	This is the in-depth chromosomal location information. </p>
		<p><a class="btn" id="collapse_btn" name="collapse_btn" data-toggle="collapse" data-target="#viewdetails_location">View details &raquo;</a></p>
        -->
	</div>
</section>

<!-- GO
================================================== -->
<section id="go">
	<div class="page-header">
		<h1>Gene Ontology</h1>
		
		<!-- Brief
		========================================== -->
		<p>This is gene ontology information.</p>

		<!-- In-depth
		========================================== 
		<p class="collapse" id="viewdetails_go">
           	This is the in-depth gene ontology information. </p>
		<p><a class="btn" id="collapse_btn" name="collapse_btn" data-toggle="collapse" data-target="#viewdetails_go">View details &raquo;</a></p>
		-->
	</div>
</section>

<!-- Phenotype
================================================== -->
<section id="phenotype">
	<div class="page-header">
		<h1>Phenotypes (<span tal:content="bioent.phenotype_count">)</h1>

		
		<!-- Brief
		========================================== -->
		<div>
			<ul class='unstyled'>
				<table class='table table-striped table-bordered table-condensed tablesorter' id='phenotype_table'>
			<thead>
  			<tr>
  				<th>Phenotype-Bioent Relationship</th>
    			<th>Phenotype Name</th>
    			<th>Evidence Count</th>

  			</tr>
  			</thead>
  			<tbody>
  			<tr tal:repeat="bioent_biocon bioent.phenotypes">
    			<td><a tal:attributes="href bioent_biocon.link" tal:content="bioent_biocon.name"></a></td>			
    			<td><a tal:attributes="href bioent_biocon.biocon.link" tal:content="bioent_biocon.biocon.name"></a></td>					
    			<td><span tal:content="bioent_biocon.evidence_count"></span> <span class="muted" style="float:right" tal:condition="python: bioent_biocon.evidence_desc" tal:content="'(' + bioent_biocon.evidence_desc + ')'"></span></td>

  			</tr>
  			</tbody>
			</table>
			</ul>
		</div>

		<!-- In-depth
		========================================== 
		<p class="collapse" id="viewdetails_phenotype">
           	This is the in-depth phenotype information. </p>
		<p><a class="btn" id="collapse_btn" name="collapse_btn" data-toggle="collapse" data-target="#viewdetails_phenotype">View details &raquo;</a></p>
	-->
	</div>
</section>

<!-- Interactions
================================================== -->
<section id="interactions">
	<div class="page-header">
		<h1>Interactions (<span tal:content="bioent.interaction_count">)</h1>
	
		<h3>Cytoscape Visualization</h3>
			<p>(Shows interactions with 3 or more pieces of evidence.)</p>
		<div id="cytoscapeweb">
            Cytoscape visualization is loading...
        </div>

        
		<!-- Brief
		========================================== -->
		<div>
			<ul class='unstyled'>
				<table class='table table-striped table-bordered table-condensed tablesorter' id='interaction_table'>
			<thead>
  			<tr>
    			<th>Interaction Name</th>
    			<th>Interactor Name</th>
    			<th># Genetic Evidence</th>
    			<th># Physical Evidence</th>
    			<th># Total Evidence</th>
  			</tr>
  			</thead>
  			<tbody>
  			<tr tal:repeat="interaction bioent.interactions">
    			<td> <a tal:attributes="href interaction.link" tal:content="interaction.name">Id</a></td>
    			<td>
    				<a tal:condition="python: interaction.source.official_name == bioent.basic_info.official_name and interaction.sink.official_name == bioent.basic_info.official_name" 
    					tal:attributes="href interaction.source.link" 
    					tal:content="interaction.source.name"></a>
    				<a tal:condition="python: interaction.source.official_name != bioent.basic_info.official_name" 
    					tal:attributes="href interaction.source.link" 
    					tal:content="interaction.source.name"></a>
    				<a tal:condition="python: interaction.sink.official_name != bioent.basic_info.official_name" 
    					tal:attributes="href interaction.sink.link" 
    					tal:content="interaction.sink.name"></a>	
    			</td>			
    			<td tal:content="interaction.genetic_evidence_count"></td>
    			<td tal:content="interaction.physical_evidence_count"></td>
    			<td tal:content="interaction.evidence_count"></td>
  			</tr>
  			</tbody>
			</table>
			</ul>
		</div>

		<!-- In-depth
		==========================================
		<div class="collapse" id="viewdetails_interactions">
			This is in-depth interaction information.
        </div>
		<p><a class="btn" id="collapse_btn" name="collapse_btn" data-toggle="collapse" data-target="#viewdetails_interactions">View details &raquo;</a></p>
		 -->
	</div>
</section>

<!-- Expression
================================================== -->
<section id="expression">
	<div class="page-header">
		<h1>Expression</h1>
		
		<!-- Brief
		========================================== -->
		<p>This is expression information.</p>

		<!-- In-depth
		========================================== 
		<p class="collapse" id="viewdetails_expression">
           	This is the in-depth expression information. </p>
		<p><a class="btn" id="collapse_btn" name="collapse_btn" data-toggle="collapse" data-target="#viewdetails_expression">View details &raquo;</a></p>
		-->
	</div>
</section>

<!-- Wiki
================================================== -->
<section id="wiki">
	<div class="page-header">
		<h1>Wiki</h1>
    	<a tal:attributes="href 'http://wiki.yeastgenome.org/index.php/' + bioent.basic_info.official_name" tal:content="'http://wiki.yeastgenome.org/index.php/' + bioent.basic_info.official_name"></a>
	</div>
</section>

<!-- History
================================================== -->
<section id="history">
	<div class="page-header">
		<h1>History</h1>
		
		<!-- Brief
		========================================== -->
		<p>This is history information.</p>

		<!-- In-depth
		========================================== 
		<p class="collapse" id="viewdetails_history">
           	This is the in-depth history information. </p>
		<p><a class="btn" id="collapse_btn" name="collapse_btn" data-toggle="collapse" data-target="#viewdetails_history">View details &raquo;</a></p>
        -->
	</div>
</section>

<!-- References
================================================== -->
<section id="literature">
	<div class="page-header">
		<h1>References</h1>
		
		<!-- Brief
		========================================== -->
		<p>This is literature information.</p>

		<!-- In-depth
		========================================== 
		<p class="collapse" id="viewdetails_literature">
           	This is the in-depth literature information. </p>
		<p><a class="btn" id="collapse_btn" name="collapse_btn" data-toggle="collapse" data-target="#viewdetails_literature">View details &raquo;</a></p>
		-->
	</div>
</section>

<script>
	$(document).ready(function() {
    	$('#interaction_table').dataTable( {
    		"sPaginationType": "bootstrap",
    		"aaSorting": [[4, "desc"]]
  		} );
  		$('#phenotype_table').dataTable( {
    		"sPaginationType": "bootstrap",
    		"aaSorting": [[2, "desc"]]
  		} );
	} );
</script>

    <script type="text/javascript">

            window.onload=function() {
                // id of Cytoscape Web container div
                var div_id = "cytoscapeweb";
                                
                // visual style we will use
                var visual_style = {
                    nodes: {
                        color: {
                            discreteMapper: {
                                attrName: "focus",
                                entries: [
                                    { attrValue: '1', value: "#fade71" }
                                ]
                            }
                        },
                        labelHorizontalAnchor: "center"
                    },
                };
                
                // initialization options
                var options = {
                    // where you have the Cytoscape Web SWF
                    swfPath: "../static/js/CytoscapeWeb",
                    // where you have the Flash installer SWF
                    flashInstallerPath: "/swf/playerProductInstall"
                };
                
                // init and draw
                var vis = new org.cytoscapeweb.Visualization(div_id, options);
                
                // you could also use other formats (e.g. GraphML) or grab the network data via AJAX
                $.get('/bioent_graph/'+"${bioent['basic_info']['official_name']}", function(data) {
                	vis.draw({ network: data, visualStyle: visual_style});

            	});
                
            };
        </script>
        
        <style>
            /* The Cytoscape Web container must have its dimensions set. */
            html, body { height: 80%; width: 100%; padding: 0; margin: 0; }
            #cytoscapeweb { width: 100%; height: 100%; }
        </style>

</div>

</body>
</div>
</div>

