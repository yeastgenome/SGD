<div metal:use-macro="layout">
  <div metal:fill-slot="content">


<body>

<!-- Subhead
================================================== -->
<header class="jumbotron subhead" id="overview">
	<div class="container">
		<h1>Chemical: <span tal:content="chemical.display_name"></h1>
	</div>
</header>


<div class="container-fluid">

<!-- Docs nav
================================================== -->
<div class="row-fluid">
	<div class="span3 bs-docs-sidebar">
		<ul class="nav nav-list bs-docs-sidenav affix-top" id='navbar' name='navbar'>
			<li><h3><span tal:content="chemical.display_name"></h3></li>
			<li><a href="#abstract"><i class="icon-chevron-right"></i> Abstract</a></li>
			<li><a href="#cited_for_annotations"><i class="icon-chevron-right"></i> Cited For Annotations</a></li>
		</ul>
	</div>
	
	<div class="span9">  

<!-- Abstract
================================================== -->
<section id="abstract">
	<div class="page-header">
		<h1>Summary</h1>
		<div>
			<p tal:condition="python: len(chemical.aliases) > 0"><strong>Aliases:</strong>
			<span tal:repeat="alias chemical.aliases">
				<span tal:content="alias.name"></span>,
			</span>
			</p>
		</div>
	</div>
</section>

<!-- Phenotypes
================================================== -->
<section id="phenotypes">
	<div class="page-header">
		<h1>Phenotypes <span id='phenotype_header'></span></h1>
		<div id='phenotype_wrapper'>
		<p id='phenotype_message' style="display:none;">No phenotype information curated from this chemical.</p>
			<table class='table table-striped table-bordered table-condensed tablesorter' id='phenotype_table' width="100%">
			<thead>
  			<tr>
    			<th>Phenotype</th>
    			<th>Gene</th>
    			<th>Qualifier</th>
    			<th>Mutant Type</th>
  			</tr>
  			</thead>
			</table>
			<br></br>
		</div>
	</div>
</section>


<script>
$(document).ready(function() {

		//Set up phenotype table
		var options = basic_datatable_options("phenotype_header", '${link_maker.phenotype_filename}', "phenotype_wrapper", "phenotype_message");
		options["aaSorting"] = [[0, "asc"]];
		options["bProcessing"] = true
		options["sAjaxSource"] = '${link_maker.phenotype_overview_table_link}';
		options["aoColumns"] = [null, null, null, null]
		options["bPaginate"] = true;
		$('#phenotype_table').dataTable(options);
	} );
</script>

</div>

</body>
</div>
</div>

