            "title": "Date Changed",
            "type": "string",
            "anyOf": [
                {
                    "format": "date-time"
                },
                {
                    "format": "date"
                }
            ]
        },
        "changed_by": {
            "title": "Changed By",
            "type": "string"
        },
	"date_archived": {
	    "title": "Date Archived",
            "type": "string",
