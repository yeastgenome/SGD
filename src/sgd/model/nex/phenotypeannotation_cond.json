{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "PhenotypeannotationCondition",
    "type": "object",
    "properties": {
        "id": {
            "title": "ID",
            "type": "integer"
        },
	"annotation_id": {
	    "title": "Annotation ID",
            "type": "integer"
	},
        "condition_class": {
            "title": "Condition Class",
            "type": "string"
        },
        "condition_name": {
            "title": "Condition Name",
            "type": "string"
        },
	"condition_value": {
            "title": "Condition Value",
            "type": "string"
        },
        "condition_unit": {
            "title": "Condition Unit",
            "type": "string"
        },
        "date_created": {
            "title": "Date Created",
            "type": "string",
            "anyOf": [
                {
                    "format": "date-time"
                },
                {
                    "format": "date"
                }
            ]
        },
        "created_by": {
            "title": "Name",
            "type": "string"
        }
    }, 
    "required": ["annotation_id", "condition_class", "condition_name", "condition_value"]
}