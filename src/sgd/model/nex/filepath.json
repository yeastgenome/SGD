{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "FilePath",
    "type": "object",
    "properties": {
        "id": {
            "title": "ID",
            "type": "integer"
        },
        "filepath": {
            "title": "filepath",
            "type": "string"
        },
        "source": {
            "title": "Source",
            "$ref": "source.json"
        },
        "date_created": {
            "title": "Date Created",
            "type": "string",
            "anyOf": [
                {
                    "format": "date-time"
                },
                {
                    "format": "date"
                }
            ]
        },
        "created_by": {
            "title": "Name",
            "type": "string"
        }
    },
    "required": ["filepath"]
}