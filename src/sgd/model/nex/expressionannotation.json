{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "ExpressionAnnotation",
    "type": "object",
    "properties": {
        "id": {
            "title": "ID",
            "type": "integer"
        },
        "bud_id": {
            "title": "Bud ID",
            "type": "integer"
        },
        "source": {
            "title": "Source",
            "$ref": "source.json"
        },
	"dbentity_id": {
	    "title": "Dbentity ID",
            "type": "integer"
	},
        "taxonomy_id": {
            "title": "Taxonomy ID",
            "type": "integer"
        },
        "reference_id": {
            "title": "Reference ID",
            "type": "integer"
        },
	"datasetsample_id": {
            "title": "DatsetSample ID",
            "type": "integer"
        },
	"expression_value": {
            "title": "Expression Value",
            "type": "number"
        },
        "date_created": {
            "title": "Date Created",
            "type": "string",
            "anyOf": [
                {
                    "format": "date-time"
                },
                {
                    "format": "date"
                }
            ]
        },
        "created_by": {
            "title": "Name",
            "type": "string"
        }
    },
    "required": ["source", "dbentity_id", "taxonomy_id", "datasetsample_id", "expression_value"]
}