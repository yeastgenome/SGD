{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Protein Domain Annotation",
    "type": "object",
    "properties": {
        "id": {
            "title": "ID",
            "type": "integer"
        },
        "bud_id": {
            "title": "Bud ID",
            "type": "integer"
        },
        "source": {
            "title": "Source",
            "$ref": "source.json"
        },
	"dbentity_id": {
	    "title": "DBENTITY ID",
            "type": "integer"
	},
        "taxonomy_id": {
            "title": "Taxonomy ID",
            "type": "integer"
        },
        "reference_id": {
            "title": "Reference ID",
            "type": "integer"
        },
	"proteindomain_id": {
            "title": "ProteinDomain ID",
            "type": "integer"
        },
        "start_index": {
            "title": "Start Index",
            "type": "integer"
        },
	"end_index": {
	    "title": "End Index",
	    "type": "integer"
	},
	"date_of_run": {
            "title": "Date of Run",
            "type": "string",
            "anyOf": [
                {
                    "format": "date-time"
                },
                {
                    "format": "date"
                }
            ]
	},
        "date_created": {
            "title": "Date Created",
            "type": "string",
            "anyOf": [
                {
                    "format": "date-time"
                },
                {
                    "format": "date"
                }
            ]
        },
        "created_by": {
            "title": "Name",
            "type": "string"
        }
    }, 
    "required": ["source", "dbentity_id", "proteindomain_id", "start_index", "end_index"]
}