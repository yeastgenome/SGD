{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "ProteinexptAnnotation",
    "type": "object",
    "properties": {
        "id": {
            "title": "ID",
            "type": "integer"
        },
        "bud_id": {
            "title": "Bud ID",
            "type": "integer"
        },
        "source": {
            "title": "Source",
            "$ref": "source.json"
        },
	"locus_id": {
	    "title": "LOCUS ID",
            "type": "integer"
	},
        "taxonomy_id": {
            "title": "Taxonomy ID",
            "type": "integer"
        },
        "reference_id": {
            "title": "Reference ID",
            "type": "integer"
        },
	"experiment_type": {
            "title": "Experiment Type",
            "type": "string"
        },	
	"data_value": {
            "title": "Data Value",
            "type": "string"
        },
	"data_unit": {
            "title": "Data Unit",
            "type": "string"
        },
	"assay_id": {
            "title": "Assay ID",
            "type": "integer"
        },
        "date_created": {
            "title": "Date Created",
            "type": "string",
            "anyOf": [
                {
                    "format": "date-time"
                },
                {
                    "format": "date"
                }
            ]
        },
        "created_by": {
            "title": "Name",
            "type": "string"
        }
    },
    "required": ["source", "locus_id", "reference_id", "experiment_type", "data_value", "data_unit"]
}