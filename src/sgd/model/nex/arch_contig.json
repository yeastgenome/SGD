{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Arch_Contig",
    "type": "object",
    "properties": {
        "id": {
            "title": "ID",
            "type": "integer"
        },
        "display_name": {
            "title": "Name",
            "type": "string"
        },
        "format_name": {
            "title": "Format Name",
            "type": "string"
        },
        "link": {
            "title": "Link",
            "type": "string"
        },
        "bud_id": {
            "title": "Bud ID",
            "type": "integer"
        },
        "source": {
            "title": "Source",
            "$ref": "source.json"
        },
        "taxonomy_id": {
            "title": "Taxonomy ID",
            "type": "integer"
        },
        "reference_chromosome": {
            "type": "object",
            "properties": {
                "display_name": {
                    "title": "Display Name",
                    "type": "string"
                }
            },
            "required": ["display_name"]
        },
        "genome_release": {
            "title": "Genome Release",
            "$ref": "genomerelease.json"
        },
        "so_id": {
            "title": "SO ID",
            "type": "integer"
        },
        "centromere_start": {
            "title": "Centromere Start",
            "type": "integer"
        },
        "centromere_end": {
            "title": "Centromere End",
            "type": "integer"
        },
        "genbank_accession": {
            "title": "Genbank Accession",
            "type": "string"
        },
        "gi_number": {
            "title": "GI Number",
            "type": "string"
        },
        "refseq_id": {
            "title": "Refseq ID",
            "type": "string"
        },
        "reference_start": {
            "title": "Reference Start",
            "type": "integer"
        },
        "reference_end": {
            "title": "Reference End",
            "type": "integer"
        },
        "reference_percent_identity": {
            "title": "Reference Percent Identity",
            "type": "number"
        },
        "reference_alignment_length": {
            "title": "Reference Alignment Length",
            "type": "integer"
        },
        "seq_version": {
            "title": "Sequence Version",
            "type": "string",
            "anyOf": [
                {
                    "format": "date-time"
                },
                {
                    "format": "date"
                }
            ]
        },
        "coord_version": {
            "title": "Coordinate Version",
            "type": "string",
            "anyOf": [
                {
                    "format": "date-time"
                },
                {
                    "format": "date"
                }
            ]
        },
        "file_header": {
            "title": "File Header",
            "type": "string"
        },
	"download_filename": {
            "title": "Download Filename",
            "type": "string"
        },
        "file_id": {
            "title": "File ID",
            "type": "integer"
        },
        "residues": {
            "title": "Residues",
            "type": "string"
        },
        "date_created": {
            "title": "Date Created",
            "type": "string",
            "anyOf": [
                {
                    "format": "date-time"
                },
                {
                    "format": "date"
                }
            ]
        },
        "created_by": {
            "title": "Name",
            "type": "string"
        },
	"date_created": {
            "title": "Date Created",
            "type": "string",
            "anyOf": [
	    	{
                    "format": "date-time"
		},
                {
                    "format": "date"
		}
            ]
	}
    },
    "required": ["display_name", "seq_version", "so_id", "residues"]
}