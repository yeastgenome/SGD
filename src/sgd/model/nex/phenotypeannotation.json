{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "PhenotypeAnnotation",
    "type": "object",
    "properties": {
        "id": {
            "title": "ID",
            "type": "integer"
        },
        "bud_id": {
            "title": "Bud ID",
            "type": "integer"
        },
        "source": {
            "title": "Source",
            "$ref": "source.json"
        },
	"dbentity_id": {
	    "title": "Dbentity ID",
            "type": "integer"
	},
        "taxonomy_id": {
            "title": "Taxonomy ID",
            "type": "integer"
        },
        "reference_id": {
            "title": "Reference ID",
            "type": "integer"
        },
	"phenotype_id": {
            "title": "Phenotype ID",
            "type": "integer"
        },
	"experiment_id": {
            "title": "Experiment ID",
            "type": "integer"
        },
	"mutant_id": {
            "title": "Mutant ID",
            "type": "integer"
        },
	"allele_id": {
            "title": "Allele ID",
            "type": "integer"
        },
	"reporter_id": {
            "title": "Repporter ID",
            "type": "integer"
        },
	"assay_id": {
            "title": "Assay ID",
            "type": "integer"
        },
	"strain_name": {
            "title": "Strain Name",
            "type": "string"
        },
	"details": {
            "title": "Details",
            "type": "string"
        },
        "date_created": {
            "title": "Date Created",
            "type": "string",
            "anyOf": [
                {
                    "format": "date-time"
                },
                {
                    "format": "date"
                }
            ]
        },
        "created_by": {
            "title": "Name",
            "type": "string"
        }
    },
    "required": ["source", "dbentity_id", "phenotype_id", "reference_id", "mutant_id", "experiment_id"]
}