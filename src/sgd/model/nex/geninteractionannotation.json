{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "PhysinteractionAnnotation",
    "type": "object",
    "properties": {
        "id": {
            "title": "ID",
            "type": "integer"
        },
        "bud_id": {
            "title": "Bud ID",
            "type": "integer"
        },
        "source": {
            "title": "Source",
            "$ref": "source.json"
        },
	"dbentity1_id": {
	    "title": "DBENTITY1 ID",
            "type": "integer"
	},
	"dbentity2_id": {
            "title": "DBENTITY2 ID",
            "type": "integer"
        },
        "taxonomy_id": {
            "title": "Taxonomy ID",
            "type": "integer"
        },
        "reference_id": {
            "title": "Reference ID",
            "type": "integer"
        },
	"phenotype_id": {
            "title": "PHENOTYPE ID",
            "type": "integer"
        },
        "biogrid_experimental_system": {
            "title": "BioGrid Experimental System",
            "type": "string"
        },
	"annotation_type": {
	    "title": "Annotation Type",
	    "type": "string"
	},
	"bait_hit": {
            "title": "Bait Hit",
            "type": "string"
        },
	"description": {
            "title": "Description",
            "type": "string"
        },
        "date_created": {
            "title": "Date Created",
            "type": "string",
            "anyOf": [
                {
                    "format": "date-time"
                },
                {
                    "format": "date"
                }
            ]
        },
        "created_by": {
            "title": "Name",
            "type": "string"
        }
    }, 
    "required": ["source", "dbentity1_id", "dbentity2_id", "phenotype_id", "reference_id", "annotation_type", "bait_hit", "biogrid_experimental_system"]
}