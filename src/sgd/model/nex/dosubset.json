{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "DoSubset",
    "type": "object",
    "properties": {
        "id": {
            "title": "ID",
            "type": "integer"
        },
        "display_name": {
            "title": "Name",
            "type": "string"
        },
        "format_name": {
            "title": "Format Name",
            "type": "string"
        },
        "link": {
            "title": "Link",
            "type": "string"
        },
	"subset_name": {
            "title": "Subset Name",
            "type": "string"
        },
	"genome_count": {
            "title": "Genome Count",
            "type": "integer"
        },
        "description": {
            "title": "Description",
            "type": "string"
        },
        "bud_id": {
            "title": "Bud ID",
            "type": "integer"
        },
        "source": {
            "title": "Source",
            "$ref": "source.json"
        },
        "do_id": {
            "title": "DO ID",
            "type": "integer"
        },
        "date_created": {
            "title": "Date Created",
            "type": "string",
            "anyOf": [
                {
                    "format": "date-time"
                },
                {
                    "format": "date"
                }
            ]
        },
        "created_by": {
            "title": "Name",
            "type": "string"
        }
    },
    "required": ["display_name", "format_name", "source", "do_id", "subset_name", "genome_count"]
}