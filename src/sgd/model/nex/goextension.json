{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "GoExtension",
    "type": "object",
    "properties": {
        "id": {
            "title": "ID",
            "type": "integer"
        },
	"annotation_id": {
	    "title": "Annotation ID",
            "type": "integer"
	},
        "group_id": {
            "title": "Group ID",
            "type": "integer"
        },
        "dbxref_id": {
            "title": "DBXREF ID",
            "type": "string"
        },
	"link": {
            "title": "LINK",
            "type": "string"
        },
        "ro_id": {
            "title": "RO ID",
            "type": "integer"
        },
        "date_created": {
            "title": "Date Created",
            "type": "string",
            "anyOf": [
                {
                    "format": "date-time"
                },
                {
                    "format": "date"
                }
            ]
        },
        "created_by": {
            "title": "Name",
            "type": "string"
        }
    }, 
    "required": ["annotation_id", "group_id", "dbxref_id", "link", "ro_id"]
}