{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Strain",
    "type": "object",
    "properties": {
        "id": {
            "title": "ID",
            "type": "integer"
        },
        "display_name": {
            "title": "Name",
            "type": "string"
        },
        "format_name": {
            "title": "Format Name",
            "type": "string"
        },
        "link": {
            "title": "Link",
            "type": "string"
        },
        "bud_id": {
            "title": "Bud ID",
            "type": "integer"
        },
        "source": {
            "title": "Source",
            "$ref": "source.json"
        },
        "date_created": {
            "title": "Date Created",
            "type": "string",
            "anyOf": [
                {
                    "format": "date-time"
                },
                {
                    "format": "date"
                }
            ]
        },
        "created_by": {
            "title": "Name",
            "type": "string"
        },
	"sgdid": {
	    "title": "SGDID",
            "type": "string"
	},
	"dbentity_status": {
            "title": "DBentity Status",
            "type": "string"
        },
	"strain_type": {
            "title": "Strain Type",
            "type": "string"
        },
	"genotype": {
            "title": "GenoType",
            "type": "string"
        },
	"genbank_id": {
            "title": "GenBank ID",
            "type": "string"
        },
	"assembly_size": {
            "title": "Assembly Size",
            "type": "number"
        },
	"fold_coverage": {
            "title": "Fold Converage",
            "type": "number"
        },
	"scafford_number": {
            "title": "Scafford Number",
            "type": "number"
        },
	"longest_scafford": {
            "title": "Longest Scafford",
            "type": "number"
        },
	"scafford_nfifty": {
            "title": "Scafford nFifty",
            "type": "number"
        },
	"feature_count": {
            "title": "Feature Count",
            "type": "number"
        },
        "summaries": {
            "title": "Summaries",
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "title": "ID",
                        "type": "integer"
                    },
                    "text": {
                        "title": "Text",
                        "type": "string"
                    },
                    "html": {
                        "title": "HTML",
                        "type": "string"
                    },
                    "summary_type": {
                        "title": "Summary Type",
                        "type": "string"
                    },
                    "source": {
                        "title": "Source",
                        "$ref": "source.json"
                    },
                    "references": {
                        "title": "References",
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "id": {
                                    "title": "ID",
                                    "type": "integer"
                                }
                            }
                        }
                    },
                    "date_created": {
                        "title": "Date Created",
                        "type": "string",
                        "anyOf": [
                            {
                                "format": "date-time"
                            },
                            {
                                "format": "date"
                            }
                        ]
                    },
                    "created_by": {
                        "title": "Created By",
                        "type": "string"
                    }
                },
                "required": ["text", "summary_type", "source"]
            }
        },
        "urls": {
            "title": "Urls",
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "title": "ID",
                        "type": "integer"
                    },
                    "display_name": {
                        "title": "Display Name",
                        "type": "string"
                    },
                    "link": {
                        "title": "Link",
                        "type": "string"
                    },
                    "url_type": {
                        "title": "Url Type",
                        "type": "string"
                    },
                    "source": {
                        "title": "Source",
                        "$ref": "source.json"
                    },
                    "date_created": {
                        "title": "Date Created",
                        "type": "string",
                        "anyOf": [
                            {
                                "format": "date-time"
                            },
                            {
                                "format": "date"
                            }
                        ]
                    },
                    "created_by": {
                        "title": "Created By",
                        "type": "string"
                    }
                },
                "required": ["display_name", "link", "url_type", "source"]
            }
        }
    },
    "required": ["display_name"]
}