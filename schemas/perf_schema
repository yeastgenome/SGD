/* Perf_Core */

DROP TABLE BIOENTITY;
CREATE TABLE BIOENTITY ( BIOENTITY_ID INTEGER NOT NULL, JSON VARCHAR2(4000) NOT NULL, CONSTRAINT BIOENTITY_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA1;

DROP TABLE LOCUSTABS;
CREATE TABLE LOCUSTABS ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT LOCUSTABS_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA2;

DROP TABLE REFERENCE;
CREATE TABLE REFERENCE ( REFERENCE_ID INTEGER NOT NULL, JSON VARCHAR2(4000) NOT NULL, CONSTRAINT REFERENCE_PK PRIMARY KEY (REFERENCE_ID) ) TABLESPACE PDATA1;

DROP TABLE REFERENCE_BIBENTRY;
CREATE TABLE REFERENCE_BIBENTRY ( REFERENCE_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT REFERENCE_BIBENTRY_PK PRIMARY KEY (REFERENCE_ID) ) TABLESPACE PDATA2;

DROP TABLE BIOCONCEPT;
CREATE TABLE BIOCONCEPT ( BIOCONCEPT_ID INTEGER NOT NULL, JSON VARCHAR2(4000) NOT NULL, CONSTRAINT BIOCONCEPT_PK PRIMARY KEY (BIOCONCEPT_ID) ) TABLESPACE PDATA1;

DROP TABLE DISAMBIG;
CREATE TABLE DISAMBIG ( DISAMBIG_ID INTEGER NOT NULL, DISAMBIG_KEY VARCHAR2(200) NOT NULL, CLASS_TYPE VARCHAR(40) NOT NULL, SUBCLASS_TYPE VARCHAR(40), OBJ_ID INTEGER NOT NULL, CONSTRAINT DISAMBIG_PK PRIMARY KEY (DISAMBIG_ID), CONSTRAINT DISAMBIG_UK UNIQUE (DISAMBIG_KEY, CLASS_TYPE, SUBCLASS_TYPE) ) TABLESPACE PDATA1;

/* Go */

DROP TABLE GO_REFERENCES;
CREATE TABLE GO_REFERENCES ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT GO_REFERENCES_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA1;

/* Interaction */

DROP TABLE INTERACTION_OVERVIEW;
CREATE TABLE INTERACTION_OVERVIEW ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT INTERACTION_OVERVIEW_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA1;

DROP TABLE INTERACTION_DETAILS;
CREATE TABLE INTERACTION_DETAILS ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT INTERACTION_DETAILS_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA2;

DROP TABLE INTERACTION_GRAPH;
CREATE TABLE INTERACTION_GRAPH ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT INTERACTION_GRAPH_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA1;

DROP TABLE INTERACTION_RESOURCES;
CREATE TABLE INTERACTION_RESOURCES ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT INTERACTION_RESOURCES_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA1;

DROP TABLE INTERACTION_REFERENCES;
CREATE TABLE INTERACTION_REFERENCES ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT INTERACTION_REFERENCES_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA1;

/* Literature */

DROP TABLE LITERATURE_OVERVIEW;
CREATE TABLE LITERATURE_OVERVIEW ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT LITERATURE_OVERVIEW_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA1;

DROP TABLE LITERATURE_DETAILS;
CREATE TABLE LITERATURE_DETAILS ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT LITERATURE_DETAILS_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA2;

DROP TABLE LITERATURE_GRAPH;
CREATE TABLE LITERATURE_GRAPH ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT LITERATURE_GRAPH_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA1;

/* Binding Site */

DROP TABLE BINDING_SITE_DETAILS;
CREATE TABLE BINDING_SITE_DETAILS ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT BINDING_SITE_DETAILS_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA1;

/* Phenotype */

DROP TABLE PHENOTYPE_REFERENCES;
CREATE TABLE PHENOTYPE_REFERENCES ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT PHENOTYPE_REFERENCES_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA1;

/* Protein */

DROP TABLE PROTEIN_DOMAIN_DETAILS;
CREATE TABLE PROTEIN_DOMAIN_DETAILS ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT PROTEIN_DOMAIN_DETAILS_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA2;

/* Regulation */

DROP TABLE REGULATION_OVERVIEW;
CREATE TABLE REGULATION_OVERVIEW ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT REGULATION_OVERVIEW_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA1;

DROP TABLE REGULATION_DETAILS;
CREATE TABLE REGULATION_DETAILS ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT REGULATION_DETAILS_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA2;

DROP TABLE REGULATION_GRAPH;
CREATE TABLE REGULATION_GRAPH ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT REGULATION_GRAPH_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA1;

DROP TABLE REGULATION_REFERENCES;
CREATE TABLE REGULATION_REFERENCES ( BIOENTITY_ID INTEGER NOT NULL, JSON CLOB NOT NULL, CONSTRAINT REGULATION_REFERENCES_PK PRIMARY KEY (BIOENTITY_ID) ) TABLESPACE PDATA1;




