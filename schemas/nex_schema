ALTER TABLE PROTEINBIOENTITY 
ADD CONSTRAINT PROTBIOENT_LOCUS_FK FOREIGN KEY (LOCUS_ID) REFERENCES BIOENTITY (BIOENTITY_ID);

DROP TABLE QUALIFIEREVIDENCE;
