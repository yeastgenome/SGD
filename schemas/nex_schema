CONSTRAINT EXPERIMENT_PK PRIMARY KEY (EXPERIMENT_ID), 
CONSTRAINT EXPT_UK UNIQUE (DISPLAY_NAME) ) TABLESPACE DATA1;
DROP SEQUENCE EXPERIMENT_SEQ;
