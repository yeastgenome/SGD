DROP TABLE AUX_INTERACTION;
CREATE TABLE AUX_INTERACTION ( 
INTERACTION_ID INTEGER NOT NULL, 
FORMAT_NAME VARCHAR2(100) NOT NULL, 
DISPLAY_NAME VARCHAR2(500) NOT NULL, 
CLASS VARCHAR2(40) NOT NULL, 
BIOENTITY1_ID INTEGER NOT NULL, 
BIOENTITY2_ID INTEGER NOT NULL, 
EVIDENCE_COUNT INTEGER NOT NULL, 
CONSTRAINT INTERACTION_PK PRIMARY KEY (INTERACTION_ID), 
CONSTRAINT INTERACTION_UK2 UNIQUE (CLASS, BIOENTITY2_ID, BIOENTITY1_ID), 
CONSTRAINT INTERACTION_UK1 UNIQUE (CLASS, FORMAT_NAME) ) TABLESPACE DATA2;
CREATE INDEX INTERACTION_BIOENT2_INDEX ON AUX_INTERACTION (BIOENTITY2_ID) TABLESPACE DATA2;
CREATE INDEX INTERACTION_BIOENT1_INDEX ON AUX_INTERACTION (BIOENTITY1_ID) TABLESPACE DATA2;

